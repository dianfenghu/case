DROP TABLE IF EXISTS `tx_setting`;
CREATE TABLE `tx_setting` (
  `Sid` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `WebName` varchar(255) DEFAULT NULL,
  `WebTitle` varchar(255) DEFAULT NULL COMMENT '网站名称',
  `Keyword` varchar(255) DEFAULT NULL COMMENT '网站关键字',
  `Description` varchar(255) DEFAULT NULL COMMENT '网站描述',
  `QQ` varchar(255) DEFAULT NULL COMMENT 'QQ',
  `Tel` varchar(255) DEFAULT NULL COMMENT '电话',
  `Url` varchar(255) DEFAULT NULL COMMENT '网址',
  `Logo` varchar(255) DEFAULT NULL COMMENT 'LOGO',
  `Status` int(2) DEFAULT '1' COMMENT '网站状态',
  `Copyright` varchar(255) DEFAULT NULL COMMENT '版权信息',
  `CaseNumber` varchar(255) DEFAULT NULL COMMENT '备案号',
  `Introduction` text COMMENT '公司简介',
  `Contact` varchar(255) DEFAULT NULL COMMENT '联系方式',
  `QRcode` varchar(255) DEFAULT NULL COMMENT '二维码',
  `Phone` varchar(255) DEFAULT NULL COMMENT '手机号',
  `PhoneWebStatus` int(1) DEFAULT '1' COMMENT '手机网站开关',
  `Email` varchar(255) DEFAULT NULL COMMENT '邮箱',
  `Address` varchar(255) DEFAULT NULL COMMENT '公司地址',
  `Keyword2` varchar(255) DEFAULT NULL,
  `contacts` varchar(128) DEFAULT NULL,
  `m_logo` char(128) DEFAULT NULL,
  `phone_style` int(10) DEFAULT '1',
  PRIMARY KEY (`Sid`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8;

